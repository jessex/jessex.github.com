---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Jessex.ch</title>
  <link type="application/atom+xml" href="http://jessex.ch/feed/index.xml" rel="self"/>
  <link type="text" href="http://jessex.ch/feed/index.xml" rel="alternate"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://jessex.ch/</id>
  <author>
    <name>Jessex.ch</name>
    <email>essexjosh@gmail.com</email>
  </author>

  {% for post in site.posts limit:5 %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="http://jessex.ch{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>http://jessex.ch{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}
 
</feed>
